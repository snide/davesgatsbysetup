    "@emotion/styled": "^10.0.15",
    "@mdx-js/mdx": "^1.3.1",
    "@mdx-js/react": "^1.3.1",
    "emotion-theming": "^10.0.14",
    "gatsby": "^2.13.73",
    "gatsby-plugin-canonical-urls": "^2.1.5",
    "gatsby-plugin-emotion": "^4.1.4",
    "gatsby-plugin-eslint": "^2.0.5",
    "gatsby-plugin-mdx": "^1.0.31",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-react-helmet": "^3.1.5",
    "gatsby-plugin-typescript": "^2.1.5",
    "gatsby-source-filesystem": "^2.1.18",
    "gatsby-source-graphql": "^2.1.11",
    "polished": "^3.4.1",
